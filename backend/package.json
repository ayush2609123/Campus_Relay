{
  "name": "campus-relay-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "serve": "NODE_ENV=production dotenv -e .env.production -- node dist/index.js",
    "build:serve": "npm run build && npm run serve",
    "typecheck": "tsc --noEmit",
    "seed:hubs": "ts-node -r dotenv/config scripts/seed-hubs.ts",
    "user:driver": "ts-node -r dotenv/config scripts/make-driver.ts",
    "smoke": "ts-node -r dotenv/config scripts/smoke.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mongoose": "^8.6.0",
    "qrcode": "^1.5.4",
    "react-leaflet": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20.19.11",
    "@types/qrcode": "^1.5.5",
    "dotenv-cli": "^10.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
